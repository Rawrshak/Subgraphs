{
  "name": "rawrshak",
  "license": "UNLICENSED",
  "author": "Rawrshak",
  "scripts": {
    "docker-up": "cd docker && docker-compose up",
    "docker-up:clean": "cd docker && rmdir /S data && docker-compose up",
    "codegen": "graph codegen",
    "codegen:rawrtoken": "npm run codegen -- subgraphs/rawrtoken.yaml",
    "build": "graph build",
    "build:rawrtoken": "graph build subgraphs/rawrtoken.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ gcbsumid/rawrshak",
    "create-local": "graph create --node http://localhost:8020/ gcbsumid/rawrshak",
    "create-local:rawrtoken": "graph create --node http://localhost:8020/ gcbsumid/rawrtoken",
    "remove-local": "graph remove --node http://localhost:8020/ gcbsumid/rawrshak",
    "remove-local:rawrtoken": "graph remove --node http://localhost:8020/ gcbsumid/rawrtoken",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 gcbsumid/rawrshak",
    "deploy-local:rawrtoken": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 gcbsumid/rawrtoken subgraphs/rawrtoken.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.20.0",
    "@graphprotocol/graph-ts": "0.20.0"
  }
}
